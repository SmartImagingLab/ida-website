<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  
  <session-config> 
  	<session-timeout>110</session-timeout> 
  </session-config> 
  
  <servlet>
    <servlet-name>saveData</servlet-name>
    <servlet-class>com.bjpowernode.ajax.web.action.SaveData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>saveData</servlet-name>
    <url-pattern>/savedata.do</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>userLogin</servlet-name>
    <servlet-class>com.bjpowernode.ajax.web.action.LoginAction2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userLogin</servlet-name>
    <url-pattern>/login2.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>getName</servlet-name>
    <servlet-class>com.bjpowernode.ajax.web.action.GetStudentName</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getName</servlet-name>
    <url-pattern>/getname.do</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>getId</servlet-name>
    <servlet-class>com.bjpowernode.ajax.web.action.GetStudentId</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getId</servlet-name>
    <url-pattern>/getid.do</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
</web-app>